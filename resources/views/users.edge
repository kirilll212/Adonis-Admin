								<td>{{ user.id }}</td>
								<td>{{ user.email }}</td>
								<td>{{ user.password }}</td>
								<td>{{ user.status }}</td>
								<td>{{ user.createdAt }}</td>
								<td>{{ user.updatedAt }}</td>
							</tr>
							@endeach
